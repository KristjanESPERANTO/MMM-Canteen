{
  "name": "mmm-canteen",
  "version": "2.6.3",
  "description": "A module for MagicMirrorÂ² to display menus from openmensa.org.",
  "keywords": [
    "magicmirror",
    "canteen",
    "openmensa"
  ],
  "homepage": "https://github.com/KristjanESPERANTO/MMM-Canteen#readme",
  "bugs": {
    "url": "https://github.com/KristjanESPERANTO/MMM-Canteen/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KristjanESPERANTO/MMM-Canteen.git"
  },
  "license": "MIT",
  "author": "Kevin (https://github.com/k-0/)",
  "contributors": [
    {
      "name": "Contributors",
      "url": "https://github.com/KristjanESPERANTO/MMM-Canteen/graphs/contributors"
    }
  ],
  "main": "MMM-Canteen.js",
  "type": "commonjs",
  "scripts": {
    "demo": "cd ../../ && MM_CONFIG_FILE=modules/MMM-Canteen/config.demo.js node --run start:wayland:dev",
    "lint": "eslint && prettier --check .",
    "lint:fix": "eslint --fix && prettier --write .",
    "prepare": "simple-git-hooks",
    "release": "commit-and-tag-version",
    "test": "node --run lint && node --run test:spelling",
    "test:spelling": "cspell ."
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "docs",
        "section": "Documentation"
      },
      {
        "type": "chore",
        "section": "Chores"
      }
    ]
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.2",
    "@eslint/markdown": "^7.5.1",
    "@stylistic/eslint-plugin": "^5.6.1",
    "cspell": "^9.4.0",
    "commit-and-tag-version": "^12.6.1",
    "eslint": "^9.39.2",
    "eslint-plugin-import-x": "^4.16.1",
    "globals": "^16.5.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "simple-git-hooks": "^2.13.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
